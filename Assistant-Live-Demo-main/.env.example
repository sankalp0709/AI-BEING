# Vite environment variables for API integration
# Copy to .env and set values; do not commit secrets.

# Base URL of your backend API (FastAPI server)
VITE_API_BASE_URL=http://localhost:8000

# Bearer token (JWT) for authenticated requests to the backend
VITE_API_TOKEN=

# Optional: Seeya feed path if using custom route
VITE_SEEYA_FEED_PATH=/seeya/feed
